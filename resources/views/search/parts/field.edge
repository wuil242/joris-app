<label for="job">
  <h2>{{title}}</h2>
  <select name="{{id}}" id="{{id}}" value="Director">
    <option value="0">Tout</option>
    @each(item in items)
      <option value="{{item.id}}" {{qs ? (qs[item] === item.name ? 'selected' : '') : ''}}>{{item.name}}</option>
    @endeach
  </select>
</label>